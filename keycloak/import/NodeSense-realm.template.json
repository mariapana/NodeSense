	"kc.user.profile.enabled": "true",
	"kc.user.profile.config": "{\"attributes\":[], \"groups\":[]}"
	},

	"requiredActions": [
	{ "alias": "VERIFY_EMAIL", "enabled": false },
	{ "alias": "UPDATE_PROFILE", "enabled": false },
